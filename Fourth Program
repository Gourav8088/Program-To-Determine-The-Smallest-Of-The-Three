#include<stdio.h>
int main( )
{
	int A,B,C;
	printf("Enter the age ofg A: ");
	scanf("%d", &A);
	printf("Enter the age of B: ");
	scanf("%d", &B);
	printf("Enter the age of C: ");
	scanf("%d", &C);

	if(A<B && A<C)
		printf("Smallest is A");
	if(B<A && B<C)
		printf("Smallest is B");
	if(C<A && C<B)
		printf("Smallest is C");
return 0;
}
